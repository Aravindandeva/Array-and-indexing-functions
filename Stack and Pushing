#Pushing into a sclass stack:
class stack :
  def __init__(self):
    self.stack=[]
  def add(self,dataval):
    if dataval not in self.stack:
      self.stack.append(dataval)
      return True
    else :
      return False

  def peek(self):
    return self.stack[-1]

astack=stack()
astack.add("Mon")
astack.peek()
print(astack.peek())
astack.add("Tue")
astack.add("wed")
print(astack.peek())
